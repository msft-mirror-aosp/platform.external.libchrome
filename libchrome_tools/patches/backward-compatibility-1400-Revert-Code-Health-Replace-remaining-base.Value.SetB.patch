From 7dc90725a5ab1ea57a91451fb79392b42c60da29 Mon Sep 17 00:00:00 2001
From: Jeff Lin <jeffulin@google.com>
Date: Fri, 5 May 2023 08:16:22 +0000
Subject: [PATCH] Revert "[Code Health] Replace remaining base.Value.SetBoolKey
 with dict Set"

This reverts commit c98236ff746146e6b39deb4d8b263be144d13b3f.
---
 base/values.cc | 4 ++++
 base/values.h  | 2 ++
 2 files changed, 6 insertions(+)

diff --git a/base/values.cc b/base/values.cc
index d74cf42c48..e0e7f76b54 100644
--- a/base/values.cc
+++ b/base/values.cc
@@ -1173,6 +1173,10 @@ Value* Value::SetKey(StringPiece key, Value&& value) {
   return GetDict().Set(key, std::move(value));
 }
 
+Value* Value::SetBoolKey(StringPiece key, bool value) {
+  return GetDict().Set(key, value);
+}
+
 Value* Value::SetIntKey(StringPiece key, int value) {
   return GetDict().Set(key, value);
 }
diff --git a/base/values.h b/base/values.h
index cc487bed20..4e2a6bdd66 100644
--- a/base/values.h
+++ b/base/values.h
@@ -785,6 +785,8 @@ class BASE_EXPORT GSL_OWNER Value {
   // ambiguities in the value type.
   //
   // DEPRECATED: Prefer `Value::Dict::Set()`.
+  Value* SetBoolKey(StringPiece key, bool val);
+  // DEPRECATED: Prefer `Value::Dict::Set()`.
   Value* SetIntKey(StringPiece key, int val);
   // DEPRECATED: Prefer `Value::Dict::Set()`.
   Value* SetStringKey(StringPiece key, StringPiece val);
