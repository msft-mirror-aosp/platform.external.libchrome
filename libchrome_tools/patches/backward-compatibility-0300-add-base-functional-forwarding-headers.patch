 base/bind.h             | 12 ++++++++++++
 base/callback.h         | 12 ++++++++++++
 base/callback_forward.h | 12 ++++++++++++
 base/callback_helpers.h | 12 ++++++++++++
 4 files changed, 48 insertions(+)
 create mode 100644 base/bind.h
 create mode 100644 base/callback.h
 create mode 100644 base/callback_forward.h
 create mode 100644 base/callback_helpers.h

diff --git a/base/bind.h b/base/bind.h
new file mode 100644
index 000000000000..721f61a9952f
--- /dev/null
+++ b/base/bind.h
@@ -0,0 +1,12 @@
