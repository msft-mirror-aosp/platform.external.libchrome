+
+#include "base/functional/bind.h"
+
+#endif  // BASE_BIND_H_
diff --git a/base/callback.h b/base/callback.h
new file mode 100644
index 000000000000..d561a3df298b
--- /dev/null
+++ b/base/callback.h
@@ -0,0 +1,12 @@
