---
 base/values.cc | 4 ++++
 base/values.h  | 4 ++++
 2 files changed, 8 insertions(+)

diff --git a/base/values.cc b/base/values.cc
index ee77160c6e..1bd9c099be 100644
--- a/base/values.cc
+++ b/base/values.cc
@@ -1355,6 +1355,10 @@ size_t Value::DictSize() const {
   return GetDict().size();
 }
 
+bool Value::DictEmpty() const {
+  return GetDict().empty();
+}
+
 bool operator==(const Value& lhs, const Value& rhs) {
   return lhs.data_ == rhs.data_;
 }
diff --git a/base/values.h b/base/values.h
index ca5253a63f..56d0c3e41e 100644
--- a/base/values.h
+++ b/base/values.h
@@ -915,6 +915,10 @@ class BASE_EXPORT GSL_OWNER Value {
   // DEPRECATED: prefer `Value::Dict::size()`.
   size_t DictSize() const;
 
+  // DEPRECATED: prefer `Value::Dict::empty()`.
+  [[deprecated("Use Value::Dict::empty() instead.")]]
+  bool DictEmpty() const;
+
   // Note: Do not add more types. See the file-level comment above for why.
 
   // Comparison operators so that Values can easily be used with standard
